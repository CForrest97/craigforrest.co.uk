{
  "name": "nextjs-template",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "npx prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "yarn lint --fix",
